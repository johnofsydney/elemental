<%= form_for @country, :html => { :multipart => true } do |f| %>



  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, :class => 'form-control', :required => true   %>
  </div>

  <div class="form-group">
    <label></label>
    <%= f.submit :class => 'btn btn-info'  %>
  </div>



<% end %>
